@model List<CustomerViewModel>

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
       <button class="CreateButton"><a href="/Home/CreateProductCategory">Create a Category</a></button>
       <table border="1" width="39%">  
        
    @foreach (var customer in Model){
        <tr>  
            <th>
                <h1>@customer.FirstName</h1>
            </th>
            <th>
                <h1>@customer.LastName</h1>
            </th>
        </tr>
    }
</div>
